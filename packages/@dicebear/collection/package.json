{
  "name": "@dicebear/collection",
  "version": "9.0.0-alpha.1",
  "description": "Avatar style collection for DiceBear",
  "keywords": [
    "avatar",
    "dicebear"
  ],
  "bugs": {
    "url": "https://github.com/dicebear/dicebear/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dicebear/dicebear.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*.js": {
      "types": "./lib/*.d.js",
      "default": "./lib/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/*.d.js"
      ]
    }
  },
  "files": [
    "LICENSE",
    "lib",
    "README.md"
  ],
  "scripts": {
    "prebuild": "del-cli lib",
    "build": "npm-run-all build:generate build:compile",
    "build:generate": "tsx ./scripts/generate-files.ts",
    "build:compile": "tsc",
    "prepublishOnly": "npm run build",
    "test": "uvu tests -i fixtures"
  },
  "dependencies": {
    "@dicebear/core": "9.0.0-alpha.1",
    "@dicebear/definitions": "9.0.0-alpha.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "change-case": "^5.4.3",
    "del-cli": "^5.0.0",
    "glob": "^10.3.12",
    "rollup": "^4.14.0",
    "tmp": "^0.2.1",
    "ts-morph": "^22.0.0",
    "tsx": "^4.7.1",
    "uvu": "^0.5.6",
    "webpack": "^5.76.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
