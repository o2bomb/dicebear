{
  "name": "@dicebear/collection",
  "version": "10.0.0-alpha.1",
  "description": "Avatar style collection for DiceBear",
  "keywords": [
    "avatar",
    "dicebear"
  ],
  "bugs": {
    "url": "https://github.com/dicebear/dicebear/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dicebear/dicebear.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*.js": {
      "types": "./lib/styles/*.d.js",
      "default": "./lib/styles/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/*.d.js"
      ]
    }
  },
  "files": [
    "LICENSE",
    "lib",
    "README.md"
  ],
  "scripts": {
    "prebuild": "del-cli lib",
    "build": "npm-run-all build:js build:ts",
    "build:js": "tsx ./scripts/build-js.ts",
    "build:ts": "tsx ./scripts/build-ts.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@dicebear/definitions": "10.0.0-beta.3"
  },
  "devDependencies": {
    "@dicebear/core": "^10.0.0-alpha.1",
    "change-case": "^5.4.3",
    "del-cli": "^5.0.0",
    "glob": "^10.3.12",
    "tsx": "^4.7.1"
  },
  "peerDependencies": {
    "@dicebear/core": "^10.0.0-alpha.0"
  },
  "engines": {
    "node": "^18.20 || >=20.10"
  },
  "publishConfig": {
    "access": "public"
  }
}
